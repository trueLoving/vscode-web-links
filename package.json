{
	"name": "web-links",
	"displayName": "Web-links",
	"description": "practice and test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"publisher": "starsky",
	"author": {
		"name": "starsky",
		"email": "1176789241@qq.com"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"url": "https://github.com/trueLoving/vscode-document-links.git"
	},
	"activationEvents": [
		"onCommand:webLinks.open",
		"onCommand:webLinks.add",
		"onCommand:webLinks.refresh",
		"onCommand:webLinks.delete",
		"onCommand:webLinks.edit",
		"onView:webLinks"
	],
	"main": "./out/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "web-links",
					"title": "web Links",
					"icon": "resources/document.svg"
				}
			]
		},
		"views": {
			"web-links": [
				{
					"id": "webLinks",
					"name": "web Links"
				},
				{
					"id": "others",
					"name": "others"
				}
			]
		},
		"menus": {
			"view/title": [
				{
					"command": "webLinks.refresh",
					"when": "view == webLinks",
					"group": "navigation"
				},
				{
					"command": "webLinks.add",
					"when": "view == webLinks"
				}
			],
			"view/item/context": [
				{
					"command": "webLinks.open",
					"when": "view == webLinks && viewItem == webLink"
				},
				{
					"command": "webLinks.delete",
					"when": "view == webLinks && viewItem == webLink"
				}
			]
		},
		"configuration": {
			"title": "webLinks",
			"properties": {
				"webLinks": {
					"type": "array",
					"default": [
						{
							"label": "vue",
							"url": "https://cn.vuejs.org/"
						},
						{
							"label": "vs code",
							"url": "https://code.visualstudio.com/"
						}
					]
				}
			}
		},
		"commands": [
			{
				"command": "webLinks.refresh",
				"title": "refresh web links",
				"icon": "resources/refresh.svg"
			},
			{
				"command": "webLinks.open",
				"title": "open web links"
			},
			{
				"command": "webLinks.add",
				"title": "add web links"
			},
			{
				"command": "webLinks.delete",
				"title": "delete web links"
			},
			{
				"command": "webLinks.edit",
				"title": "edit web links"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"package": "vsce package"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	}
}